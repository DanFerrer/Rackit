<div class="row">
  <div class="col-lg-12">
      <h1 class="page-header">Around You</h1>
  </div>
</div>
<% @spots.each do |spot| %>
  <% present spot do |spot_presenter| %>
    <div class="row">
      <div class="col-md-7">
        <%= spot_presenter.image_url %>
      </div>
      <div class="col-md-5">
        <h3><%= spot_presenter.name %></h3>
        <a class="btn btn-primary" href="#">View Project <span class="glyphicon glyphicon-chevron-right"></span></a>
      </div>
    </div>
  <hr>
  <% end %>
<% end %>
